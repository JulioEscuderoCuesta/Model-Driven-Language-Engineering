<?xml version="1.0" encoding="UTF-8"?>
<candy_factory:Factory
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:candy_factory="http://www.unican.es/ildm/candy_factory"
    xsi:schemaLocation="http://www.unican.es/ildm/candy_factory candy_factory.ecore"
    name="MyFactory">
  <elements
      xsi:type="candy_factory:CandyProducer"
      id="candyProducer1"
      output="//@elements.6"/>
  <elements
      xsi:type="candy_factory:CandyProducer"
      id="candyFlatProducer1"
      output="//@elements.9"/>
  <elements
      xsi:type="candy_factory:StickProducer"
      id="stickProducer1"
      output="//@elements.7"/>
  <elements
      xsi:type="candy_factory:Flattener"
      id="flattener"
      output="//@elements.10"
      input="//@elements.9"/>
  <elements
      xsi:type="candy_factory:Assembler"
      id="assembler1"
      output="//@elements.8"
      input="//@elements.7 //@elements.6 //@elements.10"/>
  <elements
      xsi:type="candy_factory:Wrapper"
      id="wrapper1"
      output="//@elements.11"
      input="//@elements.8"/>
  <elements
      xsi:type="candy_factory:ConveyorBelt"
      id="belt1"
      target="//@elements.4"
      source="//@elements.0">
    <component
        xsi:type="candy_factory:CandyBall"/>
  </elements>
  <elements
      xsi:type="candy_factory:ConveyorBelt"
      id="belt2"
      target="//@elements.4"
      source="//@elements.2">
    <component
        xsi:type="candy_factory:Stick"/>
  </elements>
  <elements
      xsi:type="candy_factory:ConveyorBelt"
      id="belt3"
      target="//@elements.5"
      source="//@elements.4">
    <component
        xsi:type="candy_factory:Lollipop"/>
  </elements>
  <elements
      xsi:type="candy_factory:ConveyorBelt"
      id="belt4"
      capacity="1"
      target="//@elements.3"
      source="//@elements.1">
    <component
        xsi:type="candy_factory:CandyBall"/>
  </elements>
  <elements
      xsi:type="candy_factory:ConveyorBelt"
      id="belt5"
      capacity="1"
      target="//@elements.4"
      source="//@elements.3">
    <component
        xsi:type="candy_factory:CandyFlat"/>
  </elements>
  <elements
      xsi:type="candy_factory:ConveyorBelt"
      id="lollipopOut"
      source="//@elements.5"/>
</candy_factory:Factory>
